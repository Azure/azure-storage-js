{
  "name": "@azure/storage-blob",
  "version": "10.0.0-preview",
  "description": "Microsoft Azure Storage SDK for JavaScript - Blob",
  "main": "./dist/index.js",
  "module": "./dist-esm/lib/index.js",
  "browser": "./dist/index.browser.js",
  "types": "./typings/lib/index.d.ts",
  "engines": {
    "node": ">=6.5.0"
  },
  "dependencies": {
    "ms-rest-js": "0.21.413",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "es6-promise": "^4.2.4",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-edge-launcher": "^0.4.2",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.2",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^5.0.2",
    "rollup-plugin-virtual": "^1.0.1",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "test": "npm run test:node && npm run test:browser",
    "test:node": "mocha --no-timeouts -r ts-node/register test/*.ts test/node/*.ts",
    "test:browser": "npm run build:browsertest && karma start --single-run",
    "build": "npm run build:node && npm run build:browsertest",
    "build:node": "npm run build:es6 && npm run build:umd",
    "build:umd": "rollup -c",
    "build:es6": "node ./node_modules/typescript/bin/tsc -p tsconfig.json",
    "build:browsertest": "webpack --config webpack.config.test.js && rimraf browser/test/typings",
    "build:autorest": "autorest ./swagger/README.md --typescript --use=@microsoft.azure/autorest.typescript@2.0.476",
    "prepare": "npm run build",
    "clean": "rimraf dist && rimraf dist-es6 && rimraf browser/test && rimraf browser/*.js && rimraf browser/*.zip && rimraf browser/*.log && rimraf typings && rimraf temp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/azure-storage-js.git"
  },
  "keywords": [
    "Azure",
    "Storage",
    "Blob",
    "Node.js",
    "TypeScript",
    "JavaScript",
    "Browser"
  ],
  "author": "Microsoft Corporation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Azure/azure-storage-js/issues"
  },
  "homepage": "https://github.com/Azure/azure-storage-js#readme"
}
